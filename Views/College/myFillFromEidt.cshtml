                        <label  class="col-sm-2 control-label">姓名</label>
                        <div class="col-sm-4">
                            @Html.TextBoxFor(m => m.name, new { name = "name",id = "name", @class = "form-control"})
                        </div>
                        <span class="name"></span>
                    </div>
                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-2 control-label">性别</label>
                        <div class="col-sm-2">
                            <label class="radio-inline">
                                @Html.RadioButtonFor(m => m.sex, "1", new { @name = "myradio", @id = "radio2", @class = "minimal", @checked = true }) 男
                            </label>
                            <label class="radio-inline">
                                @Html.RadioButtonFor(m => m.sex, "2", new { @name = "myradio", @id = "radio1", @class = "minimal" }) 女
                            </label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label  class="col-sm-2 control-label">身份证号</label>
                        <div class="col-sm-4">
                            @Html.TextBoxFor(m => m.identity, new { id = "sfzhm", name = "identity", @class = "form-control" })
                        </div>
                        <span class="sfzhm"></span>
                    </div>
                    <div class="form-group">
