                            <th>意向单位类别</th>
                            <th>意向岗位类别</th>
                            <th>居住地</th>
                            <th>意向工作地</th>
                            <th>操作</th>
                        </tr>
                    </thead>
                    @if (Model != null)
                    {
                        <tbody id="itemContainer">
                            <tr>
                                <td>@Model.name</td>
                                <td>@(Model.sex == "1" ? "男" : Model.sex == "2" ? "女" : "")</td>
                                <td>@Model.identity</td>
                                <td>@Model.attendSchool</td>
                                <td>@Model.entrance</td>
                                <td>@Model.graduation</td>
                                <td>@Model.education</td>
                                <td>@Model.major</td>
                                <td>@Model.mobile</td>
                                <td>@Model.contactName</td>
                                <td>@Model.contactPhone</td>
                                <td>@Model.intentionalUnit</td>
                                <td>@Model.companyType.companyName</td>
                                <td>@Model.stationType.stationName</td>
                                <td>@Model.liveWhere</td>
                                <td>@Model.wrokAddr</td>
                                <td class="nowrap flex-box">
