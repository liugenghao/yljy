                        <label for="inputPassword3" class="col-sm-2 control-label">是否提供食宿</label>
                        <div class="col-sm-2">
                            <label class="radio-inline">
                                @Html.RadioButtonFor(m => m.isAccommodate, "1", new { @name = "myradio", @id = "radio2", @class = "minimal", @checked = true }) 是
                            </label>
                            <label class="radio-inline">
                                @Html.RadioButtonFor(m => m.isAccommodate, "2", new { @name = "myradio", @id = "radio1", @class = "minimal" }) 否
                            </label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">人数</label>
                        <div class="col-sm-2">
                            @Html.TextBoxFor(m => m.numPeople, new { id = "numPeople", @class = "form-control" })
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">备注</label>
                        <div class="col-sm-5">
                            @Html.TextAreaFor(m => m.remark, 3, 4, new { id = "txtUserName", @class = "form-control" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-8">
                            <button type="submit" class="btn btn-primary">提交</button>
                            <input type="button" name="btnReturn" id="btnReturn" value="取消" class="btn default" onclick="javascript: history.back(-1);" />
                        </div>
                    </div>
                }
            </div>
        </div>
        <div class="Part-three">
            版权所有：&copy;宜昌市夷陵区劳动就业管理局
        </div>
        <!-- 弹窗提示区域开始 -->
        <div class="my-alert-mask" style="display:none;"></div>
        <div class="alert my-alert alert-info alert-dismissible" style="display:none;width:20px;">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
            <h4 class="alert-title"><i class="icon fa fa-check"></i>标题</h4>
            <div class="alert-con">
                内容
            </div>
        </div>
    </div>
    <script src="~/collegeSrc/js/jquery.min.js"></script>
    <script src="~/collegeSrc/js/bootstrap.min.js"></script>
    <script src="~/collegeSrc/js/my-alert.js"></script>
    @Html.Raw(ViewBag.js)
</body>
</html>