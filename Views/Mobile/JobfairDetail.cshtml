@model Ys.Model.Model.Recruit.JobFairMeeting
@{
    ViewBag.Title = "";
    Layout = "";
}
<!DOCTYPE html>
<html>

<head>
    <meta charset='utf-8'>
    <title></title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/mobile.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
</head>

<body>
    <div class="contatiner">
        <header>
            <h1>招聘会详情</h1>
        </header>
        <div class="main detail">
            <div class="title">
                <h2  class="blue">@Model.Name</h2>
                <div  class="d-company">主办单位：@Model.Host</div>
            </div>
            <div class="line"></div>
            <div class="content">   
                <div class="row">
                    <div class="col-xs-12">招聘类型：每月大型招聘会</div>

                </div>
                <div class="row">
                    <div class="col-xs-12">举办日期：@Model.JobFairDate.ToString("yyyy-MM-dd")</div>
                </div>

                <div class="row  desc">
                    <div class="col-xs-4">招聘会内容：</div>
                    <div class="col-xs-8"></div>
                </div>
            </div>
            <div class="line"></div>
            <div class="end">
                <div class="row">
                    <div class="col-xs-12">地址:@Model.Address</div>
                    
                </div>
                <div class="row">
                    <div class="col-xs-12">咨询电话:0717-7816006</div>
                </div>
            </div>
        </div>
    </div>

</body>
</html>

