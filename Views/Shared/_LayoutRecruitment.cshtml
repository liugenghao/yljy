                    }
                    else
                    {
                        <a href="@Url.Action("Login", "Recruiting", new { area = "" })" class="f_link" id="linkuser" target="_self"><span class="f_icons icn_user"></span><span class="hovertext">登录</span></a> 
                        <a href="@Url.Action("Register_jump", "Recruiting", new { area = "" })" class="f_link linkreg" title="注册"><span class="f_icons icn_register"></span><span class="hovertext">注册</span></a>  
                    }
                    <span class="split"></span>
                    <a href="javascript:SetHome(this, window.location);" class="f_link" title="设为首页"><span class="f_icons icn_sethome"></span><span class="hovertext">设为首页</span></a>
                    <a href="javascript:AddFavorite(window.location, document.title);" class="f_link" title="加入收藏"><span class="f_icons icn_setfav"></span><span class="hovertext">加入收藏</span></a>

                </div>
            </div>
        </div>
        <div class="header-wrap">
            <div class="section">
                <div class="logo"></div>
            </div>
        </div>
