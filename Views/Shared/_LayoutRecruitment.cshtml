<body>
    <!--头部 start-->
    <div class="header" id="header">
        <div class="topview">
            <div class="section">
                <div class="title">
                    <i></i>
                    <a href="@Url.Action("Index", "Home", new { area = "" })">夷陵就业信息网</a>
                    <b>&middot;</b>
                    <a href="@Url.Action("Index", "Recruiting", new { area = "" })">在线招聘系统</a>
                </div>
                <div class="toplink">
                    <span></span>
                    @if (Session["User"] != null)
                    {    
                        <a href="@Url.Action("Index", "Home", new { area = "Recruiting/User" })" class="f_link actived"><span class="f_icons icn_user"></span><span class="hovertext">个人中心</span></a>
                        <span class="split"></span><a href="@Url.Action("Index", "Home", new { area = "Recruiting/User" })" class="f_link"><span class="f_icons icn_msg"></span><span class="hovertext">系统消息 <i>@ViewBag.NoticeCount</i></span></a>                           
                        <span class="split"></span><a href="@Url.Action("Logout", "Recruiting", new { area = "" })" class="f_link" ><span class="f_icons icn_logout"></span><span class="hovertext">注销</span></a>  
                    }
                    else if (Session["Employer"] != null)
                    {
                        <a href="@Url.Action("Index", "Home", new { area = "Recruiting/Employer" })" class="f_link actived"><span class="f_icons icn_user"></span><span class="hovertext">个人中心</span></a>  
