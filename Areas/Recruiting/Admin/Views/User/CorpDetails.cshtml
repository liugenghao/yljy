@model Ys.Model.Model.Recruit.EmployerModel

@{
    ViewBag.Title = "用户信息编辑";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,initial-scale=1.0,user-scalable=no" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <title>用户信息编辑</title>
    <link rel="icon" href="~/favicon.ico" type="image/x-icon" />
    <link rel="shoucut icon" href="~/favicon.ico" type="image/x-icon" />
    @Styles.Render("~/Areas/Recruiting/Admin/Content/css/common")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Areas/Recruiting/Admin/Content/js/common")
</head>
<body>
    <!--main-->
    <div class="main-wrap clearfix">
        <!--导航栏-->
        <div class="crumb-wrap">
            <div class="crumb-list">
                <a href="javascript:window.parent.linkMenuTree(true, 'sysDefaultHome');"><i class="icon icon-home"></i>首页</a>
                <span class="crumb-step">&gt;</span>@Html.ActionLink("注册用户管理", "Index")
                <span class="crumb-step">&gt;</span><span>用户信息编辑</span>
            </div>
        </div>
        <!--导航栏-->        
        <div class="result-wrap">
            <!--内容-->
            <div id="floatHeadEdit" class="content-tab-wrap">
                <div class="content-tab">
                    <div class="content-tab-ul-wrap">
                        <ul>
                            <li><a class="selected" href="javascript:void(0);">用户信息</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="tab-content"> 
                <dl>
                    <dt>用户组别</dt>
                    <dd>
                        <label>企业用户</label>
                    </dd>
                </dl>               
                <dl>
                    <dt>营业执照</dt>
                    <dd>                                                         	
                        <a href="@Model.LicensePic" target="_blank"><img alt="" id="thumbnailImage" src="@Model.LicensePic" class="min-img" /></a>
                        
                    </dd>
                </dl>            
                <dl>
                    <dt>用户名</dt>
                    <dd>                        
                        <label>@Model.Login_Id</label>
                    </dd>
                </dl>
             <dl>
                    <dt>登陆密码</dt>
                    <dd>                        
                        <label>@Model.Login_Pwd</label>
                    </dd>
                </dl>
                <dl data-role="corporation">
                    <dt>名称</dt>
                    <dd>
                        <label>@Model.Name</label>
                    </dd>
                </dl>
                <dl data-role="corporation">
                    <dt>法定代表人</dt>
                    <dd>
                        <label>@Model.Legalperson</label>
                    </dd>
                </dl>
                <dl data-role="corporation">
                    <dt>所属行业</dt>
                    <dd>
                        <label>@Model.Industry.Name</label>
                    </dd>
                </dl>
                <dl data-role="corporation">
                    <dt>规 模</dt>
                    <dd>
                        <label>@Model.Scale.Name</label>
                    </dd>
                </dl>
                <dl data-role="corporation">
                    <dt>单位类型</dt>
                    <dd>
                        <label>@Model.Etype.Name</label>
                    </dd>
                </dl>
                <dl>
                    <dt>经济类型</dt>
                    <dd>
                        <label>@Model.Eco_Type.Name</label>
                    </dd>
                </dl>
                <dl>
                    <dt>注册资金</dt>
                    <dd>
                        <label>@Model.Fund 万元</label>
                    </dd>
                </dl>
                <dl>
                    <dt>联系地址</dt>
                    <dd>
                        <label>@Model.Address</label>
                    </dd>
                </dl>
                 <dl>
                    <dt>联系人</dt>
                    <dd>                        
                        <label>@Model.Contact</label>
                    </dd>
                </dl>
                <dl>
                    <dt>联系电话</dt>
                    <dd>                        
                        <label>@Model.ContactPhone</label>
                    </dd>
                </dl>
            
                <dl>
                    <dt>备 注</dt>
                    <dd>
                        <label>@Model.Inroduce</label>
                    </dd>
                </dl>
            
            </div>
            <!--/内容-->
            <!--工具栏-->
            <div class="page-footer">
                <div class="btn-wrap">
                    <a class="btn default" href="@Url.Action("CorpDetailsEdit", new { id = @Model.Id })">编辑</a>
                    <input type="button" name="btnReturn" id="btnReturn" value="返回上一页" class="btn yellow" onclick="javascript: history.back(-1);"/>

                    @*<a class="btn yellow" href="@Url.Action("CorporationIndex")">返回上一页</a>*@

                </div>
            </div>
            <!--/工具栏-->
        </div>
    </div>
    <!--/main-->
    <!-- Recommended script positioning -->
    @Scripts.Render("~/Recruiting/Admin/js/layoutMain")
    <script type="text/javascript">        
        /* document.ready */
        $(function () {
           
        });        
    </script>
</body>
</html>
