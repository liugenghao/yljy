                        <dt>是否提供食宿</dt>
                        <dd>
                            <div class="radio">
                                <label>
                                    @Html.RadioButtonFor(m => m.isAccommodate, "1", new { @name = "myradio", @id = "radio2", @class = "minimal", @checked = true }) 是
                                </label>
                                <label>
                                    @Html.RadioButtonFor(m => m.isAccommodate, "2", new { @name = "myradio", @id = "radio1", @class = "minimal" }) 否
                                </label>
                            </div>
                        </dd>
                    </dl>
                    <dl>
                        <dt>人数</dt>
                        <dd>
                            @Html.TextBoxFor(m => m.numPeople, new { id = "numPeople", @class = "input normal", datatype = "*1-10000", nullmsg = "请输入人数", sucmsg = "" })
                            <span class="Validform_checktip">*</span>
                        </dd>
                    </dl>
                    <dl>
                        <dt>备注</dt>
                        <dd>
                            @Html.TextAreaFor(m => m.remark, 3, 4, new { id = "txtUserName", @class = "input normal"})
                          
                        </dd>
                    </dl>
                </div>
                <!--/内容-->
                <!--工具栏-->
                <div class="page-footer">
                    <div class="btn-wrap">
                        <input type="submit"  value="提交保存" class="btn" />
                        <input type="button" name="btnReturn" id="btnReturn" value="返回上一页" class="btn yellow" onclick="javascript: history.back(-1);" />
                        @Html.HiddenFor(m => m.id)
                    </div>
                </div>
                <!--/工具栏-->
            }
        </div>
    </div>
    <!--/main-->
    <!-- Recommended script positioning -->
    @Scripts.Render("~/Recruiting/Admin/js/layoutMain")
    @Scripts.Render("~/ueditor/js")
    <script type="text/javascript">

        /* document.ready */
        $(function () {
            /*初始化表单验证*/
            $("#form1").initValidform();

            /*编辑成功提示*/
            var userName = '@ViewData["userName"]';
            if (userName) {
                var d = parent.dialog({
                    title: "消息中心",
                    content: "用户 " + userName + " 信息编辑成功！",
                    okValue: '确定',
                    ok: function () { },
                    statusbar: "<p style='color: #999'>（对话框将在3秒后关闭）</p>"
                }).show();
                setTimeout(function () {
                    d.close().remove();
                }, 3000);
            }

            /*显示‘编辑个人信息’与取消‘编辑’事件注册*/
            var btnReturn = $("input[type='button'][name='btnReturn']");
            $(btnReturn).each(function (i, item) {
                $(this).on("click", function () {
                    $("#result-detail").toggle();
                    $("#result-edit").toggle();
                });
            });

        });
    </script>
</body>
</html>
