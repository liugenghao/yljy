                    <div class="btn-wrap">
                        <input type="button" value="提交保存" class="btn" onclick="Verification()" />
                        <input type="button" name="btnReturn" id="btnReturn" value="返回上一页" class="btn yellow" onclick="javascript: history.back(-1);" />
                        @Html.HiddenFor(m => m.id)
                    </div>
                </div>
                <!--/工具栏-->
            }
        </div>
    </div>
    <!--/main-->
    <!-- Recommended script positioning -->
    @Scripts.Render("~/Recruiting/Admin/js/layoutMain")
  
    <script type="text/javascript">
        function Verification() {
            var sfzhmValue = $("#sfzhm").val();
            var sjhValue = $("#sjh").val();
            var jjlxrdhValue =$("#jjlxrdh").val();
            var phReg = !!sjhValue.match(/^(0|86|17951)?(13[0-9]|15[012356789]|17[678]|18[0-9]|14[57])[0-9]{8}$/);
            var phjjlxrReg = !!jjlxrdhValue.match(/^(0|86|17951)?(13[0-9]|15[012356789]|17[678]|18[0-9]|14[57])[0-9]{8}$/);//手机号
            var IDcardReg = !!sfzhmValue.match(/^\d{6}(18|19|20)?\d{2}(0[1-9]|1[12])(0[1-9]|[12]\d|3[01])\d{3}(\d|X)$/i); //身份证
            if (sfzhmValue == "") {
                $(".sfzhm").text("身份证号码不能为空！").css("color", "red"); //加错误信息提示
            } else  if (!IDcardReg) {
                    $(".sfzhm").text("身份证号码格式不正确！").css("color", "red");
            } else if (sjhValue == "") {
                $(".sfzhm").text("").css("color", "#20bd00");
                $(".sjh").text("手机号不能为空！").css("color", "red"); //加错误信息提示
            } else if (!phReg) {
                $(".sjh").text("手机号格式不正确！").css("color", "red"); //加错误信息提示
            } else if (jjlxrdhValue == "") {
                $(".sjh").text("").css("color", "#20bd00");
                $(".jjlxrdh").text("紧急联系人电话不能为空！").css("color", "red"); //加错误信息提示
            } else if (!phjjlxrReg) {
                $(".jjlxrdh").text("紧急联系人电话格式不正确！").css("color", "red"); //加错误信息提示
            } else {
                $(".jjlxrdh").text("").css("color", "#20bd00");
                $("#formId").submit();
                return false;
            }
        };
        /* document.ready */
        $(function () {
            /*初始化表单验证*/
            $("#form1").initValidform();

            /*编辑成功提示*/
            var userName = '@ViewData["userName"]';
            if (userName) {
                var d = parent.dialog({
                    title: "消息中心",
                    content: "用户 " + userName + " 信息编辑成功！",
                    okValue: '确定',
                    ok: function () { },
                    statusbar: "<p style='color: #999'>（对话框将在3秒后关闭）</p>"
                }).show();
                setTimeout(function () {
                    d.close().remove();
                }, 3000);
            }

            /*显示‘编辑个人信息’与取消‘编辑’事件注册*/
            var btnReturn = $("input[type='button'][name='btnReturn']");
            $(btnReturn).each(function (i, item) {
                $(this).on("click", function () {
                    $("#result-detail").toggle();
                    $("#result-edit").toggle();
                });
            });

        });
        $.datepicker.setDefaults($.datepicker.regional['zh-CN']);
        $(".datepicker").datepicker({
            dateFormat: 'yy-mm-dd',
            changeMonth: true,
            changeYear: true,
            yearRange: "2015:2030"

        });
    </script>
</body>
</html>
