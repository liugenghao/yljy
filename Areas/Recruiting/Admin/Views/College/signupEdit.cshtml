                @Html.AntiForgeryToken()
                <!--内容-->
                <div id="floatHeadEdit" class="content-tab-wrap">
                    <div class="content-tab">
                        <div class="content-tab-ul-wrap">
                            <ul>
                                <li><a class="selected" href="javascript:void(0);">报名信息编辑</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="tab-content">
                    <dl>
                        <dt>姓名</dt>
                        <dd>
                            @Html.TextBoxFor(m => m.name, new { id = "txtUserName", @class = "input normal", datatype = "*2-5", nullmsg = "请输入姓名", sucmsg = "" })
                            <span class="Validform_checktip">*</span>
                        </dd>
                    </dl>
                    <dl>
                        <dt>性别</dt>
                        <dd>
                            <div class="radio">
                                <label>
                                    @Html.RadioButtonFor(m => m.sex, "1", new { @name = "myradio", @id = "radio2", @class = "minimal", @checked = true }) 男
                                </label>
                                <label>
                                    @Html.RadioButtonFor(m => m.sex, "2", new { @name = "myradio", @id = "radio1", @class = "minimal" }) 女
                                </label>
                            </div>
                        </dd>
                    </dl>
                    <dl>
                        <dt>身份证号</dt>
                        <dd>
                            @Html.TextBoxFor(m => m.identity, new { id = "sfzhm", @class = "input normal", datatype = "*15-18", nullmsg = "请输入身份证号", sucmsg = "" })
                            <span class="Validform_checktip sfzhm" >*</span>
                        </dd>
                    </dl>
                    <dl>
                        <dt>就读学校</dt>
                        <dd>
                            @Html.TextBoxFor(m => m.attendSchool, new { id = "txtUserName", @class = "input normal", datatype = "*2-50", nullmsg = "请输入就读学校", sucmsg = "" })
                            <span class="Validform_checktip">*</span>
                        </dd>
                    </dl>
                    <dl>
                        <dt>入学时间</dt>
                        <dd>
                            @Html.TextBox("StartDate", DateTime.Now.ToShortDateString(), new { @class = "form-control classPeriodStart editable datepicker", disabled = "disabled" })
                            
                            @*@Html.TextBoxFor(m => m.entrance, new { id = "entrance", @class = "input Wdate", onfocus = "WdatePicker({dateFmt:'yyyy-MM-dd'})" })*@
                            <span class="Validform_checktip">*</span>
                        </dd>
                    </dl>
                    <dl>
                        <dt>毕业时间</dt>
                        <dd>
                            @Html.TextBox("EndDate", DateTime.Now.ToShortDateString(), new { @class = "form-control classPeriodEnd editable datepicker", disabled = "disabled" })

                            @*@Html.TextBoxFor(m => m.graduation, new { id = "graduation", @class = "input Wdate", onfocus = "WdatePicker({dateFmt:'yyyy-MM-dd'})" })*@
                            <span class="Validform_checktip">*</span>
                        </dd>
                    </dl>
                    <dl>
                        <dt>学历</dt>
                        <dd>
                            <select id="education" name="education" class="form-control" style="width:40%;">
                                @if (Model != null)
                                {
                                    if (Model.education == "大专")
                                    {
                                        <option value="大专" selected>大专</option>
                                        <option value="本科">本科</option>
                                        <option value="硕士">硕士</option>
                                        <option value="博士">博士</option>
                                    }
                                    else if (Model.education == "本科")
